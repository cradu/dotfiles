[REPORTS]
disable=C0103,C0111,C0112,E1101,E1135,E1136,E1137,R0201,R0903,W0221,W0702,W0703

[FORMAT]
max-line-length=119
max-module-lines=400

[MISCELLANEOUS]
notes=FIXME,NOTE,XXX,TODO

[TYPECHECK]
ignore-mixin-members=yes

[DESIGN]
max-branchs=14
max-args=12
max-attributes=14
max-locals=35
max-parents=15
max-returns=15
